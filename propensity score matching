install.packages('Matchlt')
library('Matchlt')
mydata <- read.csv("Desktop/meta_teletherapy/PSM2018202102.csv")
set.seed(1234)
mydata$YEAR <- factor(mydata$YEAR, levels=c(0,1))
m.out <- matchit(YEAR~ age + education + religion + sex +  major,data = mydata, method ="nearest", ratio=4,caliper=0.001)
m.data7 <- match.data(m.out)
write.csv (m.data7, file = "Desktop/meta_teletherapy/2018202102match_nearest.csv")
plot (m.out,type = "hist")
